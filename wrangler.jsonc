{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "moltclans",
  "main": "party/main.ts",
  "compatibility_date": "2024-12-01",
  "assets": {
    "directory": "./dist",
    "not_found_handling": "single-page-application"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "main",
        "class_name": "MoltClansServer"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["MoltClansServer"]
    }
  ]
}
